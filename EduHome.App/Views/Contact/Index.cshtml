@model ContactViewModel

<div class="banner-area-wrapper">
	<div class="banner-area text-center">
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
					<div class="banner-content-wrapper">
						<div class="banner-content">
							<h2>contact</h2>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>


<div class="map-area">
	<!-- google-map-area start -->
	<div class="google-map-area">
		<!--  Map Section -->
		<div id="contacts" class="map-area">
			<div id="googleMap" style="width:100%;height:440px;"></div>
		</div>
	</div>
</div>

<div class="contact-area pt-150 pb-140">
	<div class="container">
		<div class="row">
			<div class="col-md-5 col-sm-5 col-xs-12">
				<div class="contact-contents text-center">
					 <div class="single-contact mb-65">
                                <div class="contact-icon">
                                    <img src="~/assets/img/contact/contact1.png" alt="contact">
                                </div>
                                <div class="contact-add">
                                    <h3>address</h3>
                                    <p>@Model.Setting?.Address</p>
                                    <p>@Model.Setting?.CityCountry</p>
                                </div>
                            </div>
                            <div class="single-contact mb-65">
                                <div class="contact-icon">
                                    <img src="~/assets/img/contact/contact2.png" alt="contact">
                                </div>
                                <div class="contact-add">
                                    <h3>Phone</h3>
                                    <p>@Model.Setting?.Phone1</p>
                                    <p>@Model.Setting?.Phone2</p>
                                </div>
                            </div>
                            <div class="single-contact">
                                <div class="contact-icon">
                                    <img src="~/assets/img/contact/contact3.png" alt="contact">
                                </div>
                                <div class="contact-add">
                                    <h3>Email</h3>
                                    <p>@Model.Setting?.Email1</p>
                                    <p>@Model.Setting?.Email2</p>
                                </div>
                            </div>
				</div>
			</div>
			<div class="col-md-7 col-sm-7 col-xs-12">
				<div class="reply-area">
					<h3>LEAVE A message</h3>
					<p>I must explain to you how all this a mistaken idea of ncing great explorer of the rut the is lder of human happinescias unde omnis iste natus error sit volptatem </p>
					<form asp-action="sendmessage" asp-controller="contact" method="post" enctype="multipart/form-data">
						<div class="row">

							<div class="form-group">
								<label asp-for="@Model.Message.Name">Name</label>
								<input name="name" type="text" class="form-control">
								<span class="text text-danger" asp-validation-for="@Model.Message.Name"></span>
							</div>

							<div class="form-group">
								<label asp-for="@Model.Message.Email">Email</label>
								<input name="email" type="email" class="form-control">
								<span class="text text-danger" asp-validation-for="@Model.Message.Email"></span>
							</div>

							<div class="form-group">
								<label asp-for="@Model.Message.Subject">Subject</label>
								<input name="subject" class="form-control">
								<span class="text text-danger" asp-validation-for="@Model.Message.Subject"></span>
							</div>

							<div class="form-group">
								<label asp-for="@Model.Message.Message">Message</label>
								<input name="message" class="form-control">
								<span class="text text-danger" asp-validation-for="@Model.Message.Message"></span>
							</div>

						</div>
						<button class="btn btn-success"><span>Send Message</span></button>
						<p class="form-messege"></p>
					</form>

				</div>
			</div>

		</div>
	</div>
</div>