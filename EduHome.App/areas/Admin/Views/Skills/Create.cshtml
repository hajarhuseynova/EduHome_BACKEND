@model Skills
<div class="container">
	<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		    <label asp-for="@Model.Name" >Name:</label>
			<input type="text" class="form-control" asp-for="@Model.Name"/>
			<span class="text text-danger" asp-validation-for="@Model.Name"></span>
	</div>
	<div class="form-group">
		    <label asp-for="@Model.Percent" >Percent:</label>
			<input type="text" class="form-control" asp-for="@Model.Percent"/>
			<span class="text text-danger" asp-validation-for="@Model.Percent"></span>
	</div>



	    <div class="form-group">
            <select class="form-select" asp-for="TeacherId" aria-label="Disabled select example">
                <option selected>Choose Employee</option>
                @foreach (var item in ViewBag.Teacher)
                {
                    <option value="@item.Id">@item.FullName</option>
                }

            </select>
            <span class="alert-danger" asp-validation-for="TeacherId"></span>
        </div>




			@Html.AntiForgeryToken()
	<button type="submit" class="btn btn-info">Create</button>
	</form>
</div>