@model NoticeBoard
<div class="container">
	<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		    <label asp-for="@Model.VideoLink" >Link:</label>
			<input type="text" class="form-control" asp-for="@Model.VideoLink"/>
			<span class="text text-danger" asp-validation-for="@Model.VideoLink"></span>
	</div>
	<div class="form-group">
		    <label asp-for="@Model.Description" >Desc:</label>
			<input type="text" class="form-control" asp-for="@Model.Description"/>	
			<span class="text text-danger" asp-validation-for="@Model.Description"></span>
    </div>

			@Html.AntiForgeryToken()
	<button type="submit" class="btn btn-info">Update</button>
	</form>
</div>