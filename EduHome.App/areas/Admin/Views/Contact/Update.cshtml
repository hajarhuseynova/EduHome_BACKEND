@model Contact
<div class="container">
	<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		    <label asp-for="@Model.Address" >Address:</label>
			<input type="text" class="form-control" asp-for="@Model.Address"/>
			<span class="text text-danger" asp-validation-for="@Model.Address"></span>
	</div>
			<div class="form-group">
		    <label asp-for="@Model.CityContry" >CityContry:</label>
			<input type="text" class="form-control" asp-for="@Model.CityContry"/>
			<span class="text text-danger" asp-validation-for="@Model.CityContry"></span>
	</div>

		<div class="form-group">
		    <label asp-for="@Model.FormFile" >File:</label>
			<input type="file" class="form-control" asp-for="@Model.FormFile"/>
			<span class="text text-danger" asp-validation-for="@Model.FormFile"></span>
    </div>
	
			@Html.AntiForgeryToken()
	<button type="submit" class="btn btn-info">Update</button>
	</form>
</div>